MSVC_ENABLE_PGO := 1
LIBXUL_LIBRARY   = 1
FORCE_STATIC_LIB = 1

LOCAL_INCLUDES	= \
		-I$(topsrcdir)/intl/uconv/src \
		$(NULL)

endif


include $(topsrcdir)/config/config.mk
include $(topsrcdir)/ipc/chromium/chromium-config.mk
include $(topsrcdir)/config/rules.mk

DEFINES += -DIMPL_NS_NET
