DEPTH     = @DEPTH@
topsrcdir = @top_srcdir@
srcdir    = @srcdir@
VPATH     = @srcdir@
relativesrcdir = @relativesrcdir@

include $(DEPTH)/config/autoconf.mk

# export required interfaces, even if --disable-cookies has been given
FAIL_ON_WARNINGS := 1

ifdef NECKO_COOKIES
