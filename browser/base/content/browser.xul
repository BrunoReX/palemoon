#ifdef APPMENU_ON_TABBAR
             defaultset="appmenu-toolbar-button,tabbrowser-tabs,new-tab-button,tabview-button,alltabs-button,tabs-closebutton"
#else
             defaultset="tabbrowser-tabs,new-tab-button,tabview-button,alltabs-button,tabs-closebutton"
#endif
             collapsed="true">

#ifdef APPMENU_ON_TABBAR
      <toolbarbutton id="appmenu-toolbar-button"
                     class="chromeclass-toolbar-additional"
                     type="menu"
                     label="&brandShortName;"
                     tooltiptext="&appMenuButton.tooltip;">
#include browser-appmenu.inc
      </toolbarbutton>
#endif

      <tabs id="tabbrowser-tabs"
            class="tabbrowser-tabs"
            tabbrowser="content"
